String name = "Bob";
String object = "bed";
String action = "bumped";
String emote = "cried";
String action2 = "himself to sleep";
String adjective = "weird";
String noun = "dream";
String feeling = "confused";
String feeling2 = "hungry";
int numberofdogs = 34;
int numberofbirds = 23;
int numberoftrout = 12;
float slicesofcake = 7.2;
float piecesofpie = 5.6;
float eggplantparmesan = 4.2;
//bobs dream
println(name , "is super tired");
println(name , "went to" , object);
println(name , action , "his head");
println(name, emote, "all night long");
println(name, emote, action2);
println(name, "had a", adjective, noun);
println(name, "had a", noun, "about", numberofdogs, "dogs");
println(name, "then had a", noun, "about", numberofbirds, "birds");
println(name, "had one last", noun, "about", numberoftrout, "trout");
println(name, "was very", feeling);
println(name, "had another", noun);
println(name, "had a", noun, "about", slicesofcake, "slices of cake");
println(name, "then had a", noun, "about", piecesofpie, "pieces of pie");
println(name, "had one last", noun, "about", eggplantparmesan, "egg plant parmesans");
println(name, "is now", feeling, "and", feeling2);
